{
    "name": "dynamic/silverstripe-carousel",
    "description": "A simple carousel for Silverstripe websites.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "require": {
        "php": "^8.1",
        "jonom/focuspoint": "^5",
        "nathancox/embedfield": "^3.0",
        "silverstripe/linkfield": "^5",
        "silverstripe/recipe-cms": "^6",
        "symbiote/silverstripe-gridfieldextensions": "^5",
        "unclecheese/display-logic": "^3.0"
    },
    "require-dev": {
        "dnadesign/silverstripe-elemental": "^6",
        "silverstripe/recipe-testing": "^4",
        "silverstripe/standards": "^1",
        "squizlabs/php_codesniffer": "^3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Carousel\\": "src/",
            "Dynamic\\Carousel\\Test\\": "tests/"
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/dynamic"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
