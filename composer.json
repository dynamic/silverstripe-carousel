{
    "name": "dynamic/silverstripe-carousel",
    "description": "A simple carousel for Silverstripe websites.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "require": {
        "embed/embed": "^4.0",
        "jonom/focuspoint": "^5",
        "silverstripe/linkfield":"^3.0",
        "silverstripe/recipe-cms": "^5.0",
        "symbiote/silverstripe-gridfieldextensions": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Carousel\\": "src/",
            "Dynamic\\Carousel\\Test\\": "tests/"
        }
    }
}
